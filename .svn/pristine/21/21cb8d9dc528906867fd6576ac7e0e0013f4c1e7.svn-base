package com.ucll.r0452425_tasks.domain.service;

/**
 *
 * @author Tom
 */
public class NotLoggedInException extends Exception {

    /**
     * Creates a new instance of <code>NotLoggedInExceptio</code> without detail
     * message.
     */
    public NotLoggedInException() {
    }

    /**
     * Constructs an instance of <code>NotLoggedInExceptio</code> with the
     * specified detail message.
     *
     * @param msg the detail message.
     */
    public NotLoggedInException(String msg) {
        super(msg);
    }
}

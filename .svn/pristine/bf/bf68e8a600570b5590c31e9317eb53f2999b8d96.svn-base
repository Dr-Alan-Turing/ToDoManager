package com.ucll.r0452425_tasks.domain.service;

/**
 *
 * @author Tom
 */
public class UnauthorisedActionException extends Exception {

    /**
     * Creates a new instance of <code>UnauthorisedActionException</code>
     * without detail message.
     */
    public UnauthorisedActionException() {
    }

    /**
     * Constructs an instance of <code>UnauthorisedActionException</code> with
     * the specified detail message.
     *
     * @param msg the detail message.
     */
    public UnauthorisedActionException(String msg) {
        super(msg);
    }
}
